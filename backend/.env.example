# Azure ML Configuration
# Copy this file to .env and fill in your values

# Azure ML Batch Endpoint URL
AZURE_ML_ENDPOINT=https://sales-forecast-endpoint-v2.germanywestcentral.inference.ml.azure.com/jobs

# Azure Blob Storage (for input data)
BLOB_STORAGE_ACCOUNT=your_storage_account_name
BLOB_CONTAINER=data

# Optional: Azure Service Principal credentials
# If not set, DefaultAzureCredential will use other methods (az login, managed identity, etc.)
# AZURE_CLIENT_ID=your_client_id
# AZURE_CLIENT_SECRET=your_client_secret
# AZURE_TENANT_ID=your_tenant_id
